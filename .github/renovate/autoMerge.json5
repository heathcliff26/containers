{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Auto merge Bitwarden CLI",
      "matchPackagePatterns": ["bitwarden/clients"],
      "matchUpdateTypes": ["major", "minor", "patch"],
      "automerge": true,
      "automergeType": "branch"
    },
  ]
}
